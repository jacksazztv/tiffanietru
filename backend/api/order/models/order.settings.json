{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Digital",
        "Physical",
        "TarotReading"
      ],
      "default": "Digital"
    },
    "portfolios": {
      "collection": "portfolio"
    }
  }
}
